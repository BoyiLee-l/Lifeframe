# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'Lifelog' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Lifelog
 pod 'Firebase/Core'
 pod 'Firebase/Auth'
 pod 'Firebase/Database' 
 pod 'Firebase/Storage'

 # Pods for Facebook
 pod 'FBSDKLoginKit'

 # Pods for Google Sign In
 pod 'GoogleSignIn'

 # Pods for ImagePicker
 pod 'ImagePicker'
 pod 'Kingfisher'

 pod 'Alamofire'
 pod 'SwiftyJSON', '~> 4.0'
 pod 'SVProgressHUD'
 pod 'SCLAlertView'
 pod 'MarqueeLabel'
 pod 'NVActivityIndicatorView'
 pod 'SnapKit', '~> 5.0.0'
end
